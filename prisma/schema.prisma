generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
}

model User {
  id            String    @id
  name          String?
  email         String?   @unique
}

model Prepaid {
  id            String    @id
  email         String   @unique
  address       String   @unique
}